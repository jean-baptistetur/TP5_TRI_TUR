#include <stdio.h>

typedef struct {
    char nom[50];
    char prenom[50];
    int matricule;
    float moyenne;
} Etudiant;

void triInsertion(Etudiant tab[], int N) {
    for (int i = 1; i < N; i++) {
        Etudiant temp = tab[i];
        int j = i - 1;
        while (j >= 0 && strcmp(tab[j].nom, temp.nom) > 0) {
            tab[j + 1] = tab[j];
            j--;
        }
        tab[j + 1] = temp;
    }
}

int main() {
    Etudiant etudiants[100];
    int N = 2;

    for (int i = 0; i < N; i++) {
        printf("Etudiant %d\n", i + 1);
        printf("Nom: ");
        scanf_s("%s", etudiants[i].nom, sizeof(etudiants[i].nom));
        printf("Prenom: ");
        scanf_s("%s", etudiants[i].prenom, sizeof(etudiants[i].prenom));
        printf("Matricule: ");
        scanf_s("%d", &etudiants[i].matricule);
        printf("Moyenne: ");
        scanf_s("%f", &etudiants[i].moyenne);
    }

    triInsertion(etudiants, N);

    printf("\nListe des etudiants triee par nom alphabetique:\n");
    for (int i = 0; i < N; i++) {
        printf("Nom: %s, Prenom: %s, Matricule: %d, Moyenne: %.2f\n",
            etudiants[i].nom, etudiants[i].prenom, etudiants[i].matricule, etudiants[i].moyenne);
    }

    return 0;
}
